<Window x:Class="BussinessSolution.Print.PrinterDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PrinterDialog" Height="300" Width="300">
    <Grid Loaded="Grid_Loaded">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            
        </Grid.ColumnDefinitions>
        <Label x:Name="numOfBlankLabel" Content="Num of Blank Label" Grid.Row="0" Grid.Column="0" />
        <Label  x:Name="numOfCopyLabel" Content="Num of Copies" Grid.Row="1" Grid.Column="0" />
        <Label  x:Name="printerLabel" Content="Num of Blank Label" Grid.Row="2" Grid.Column="0" />
        <TextBox x:Name="numOfBlankLabelTextbox" Grid.Row="0" Grid.Column="1" Margin="5"/>
        <TextBox x:Name="numOfCopyTextbox" Grid.Row="1" Grid.Column="1" Margin="5" />
        <ComboBox x:Name="printerCombobox" Grid.Row="2" Grid.Column="1" Margin="5" />
        <Button x:Name="printButton" Content="Print" Click="printButton_Click" Grid.Row="3" Grid.Column="1"  Margin="15" />
    </Grid>
</Window>
